<?xml version="1.0" encoding="utf-8"?>
<Test description="Unpacks 'PatchOperationFindMod > match[PatchOperationSequence]'.">

	<Patch>

		<Operation Class="PatchOperationFindMod">
			<mods>
				<li>Foo</li>
			</mods>
			<match Class="PatchOperationSequence">
				<operations>

					<li Class="PatchOp">
						foobar
					</li>

				</operations>
			</match>
		</Operation>

	</Patch>

	<Patch>

		<Operation Class="PatchOp">
			foobar
		</Operation>

	</Patch>

</Test>